{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler --local --persist",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "wrangler": "wrangler pages dev ./public",
    "generate": "drizzle-kit generate:sqlite --schema=app/db/schema.ts",
    "up": "drizzle-kit up:sqlite"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.16.1",
    "@remix-run/cloudflare-pages": "^1.16.1",
    "@remix-run/react": "^1.16.1",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "drizzle-orm": "^0.25.4",
    "geoip-country": "^4.1.57",
    "isbot": "^3.6.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
    "ua-parser-js": "^1.0.35",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.16.1",
    "@remix-run/eslint-config": "^1.16.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/ua-parser-js": "^0.7.36",
    "better-sqlite3": "^8.3.0",
    "drizzle-kit": "^0.17.6",
    "eslint": "^8.40.0",
    "miniflare": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=16.13"
  }
}
